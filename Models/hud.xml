 <animation>
   <type>translate</type>
   <object-name>NDhdg</object-name>
   <property>sim/current-view/y-offset-m[0]</property>
 <interpolation>
    <entry>
      <ind>0.64</ind>
      <dep>-2</dep>
    </entry>
    <entry>
      <ind>4.64</ind>
      <dep>2</dep>
    </entry>
  </interpolation>
