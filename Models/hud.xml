<PropertyList>
 <path>hud.ac</path>

 <animation>
   <type>select</type>
   <object-name>Pedestal.001</object-name>
   <condition>
   <equals>
    <property>sim/current-view/view-number</property>
    <value>0</value>
   </equals>
  </condition>
 </animation>

<animation>
   <type>select</type>
   <object-name>FDbars</object-name>
   <condition>
     <and>
       <equals>
         <property>/sim/hud/visibility[0]</property>
         <value>true</value>
       </equals>
       <equals>
         <property>/instrumentation/flightdirector/fd-on</property>
         <value>1</value>
       </equals>
      <not>
      <property>sim/crashed</property>
      </not>
      <equals>
       <property>sim/current-view/view-number</property>
       <value>0</value>
      </equals>
     </and>
   </condition>
 </animation>

